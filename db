CREATE TABLE categories (
    category_id SERIAL PRIMARY KEY,
    category_name VARCHAR(255) UNIQUE,
    description TEXT,
    image_url TEXT
);

CREATE TABLE products (
    product_id SERIAL PRIMARY KEY,
    product_name VARCHAR(255) UNIQUE,
    description TEXT,
    category_id INT REFERENCES categories(category_id),
    image_url VARCHAR(500),
    images TEXT[],
    created_at TIMESTAMP,
    updated_at TIMESTAMP
);
ALTER TABLE products
ALTER COLUMN images SET DATA TYPE text[] USING images::text[];


CREATE TABLE users (
    user_id SERIAL PRIMARY KEY,
    username VARCHAR(255) UNIQUE,
    email VARCHAR(255) UNIQUE,
    password VARCHAR(255),
    full_name VARCHAR(255),
    phone_number VARCHAR(50),
    address TEXT,
    role VARCHAR(50),
    created_at TIMESTAMP,
    updated_at TIMESTAMP
);
ALTER TABLE users
ALTER COLUMN date_of_birth TYPE DATE
USING date_of_birth::date;



CREATE TABLE stores (
    store_id SERIAL PRIMARY KEY,
    store_name VARCHAR(255),
    address TEXT,
    phone_number VARCHAR(50),
    created_at TIMESTAMP,
    updated_at TIMESTAMP
);

CREATE TABLE stores_products (
	stores_products_id SERIAL PRIMARY KEY,
    store_id INT REFERENCES stores(store_id),
    product_id INT REFERENCES products(product_id),
    stock_quantity INT,  -- S·ªë l∆∞·ª£ng t·ªìn kho t·∫°i c·ª≠a h√†ng
    rating NUMERIC(2, 1),
    view INT,
    sold INT,
    price_before_discount NUMERIC(12, 2),
    price NUMERIC(12, 2) -- Gi√° b√°n c·ªßa m√≥n ƒÉn t·∫°i c·ª≠a h√†ng
);


CREATE TABLE purchases (
	purchase_id SERIAL PRIMARY KEY,
    user_id INT REFERENCES users(user_id),
    stores_products_id INT REFERENCES stores_products(stores_products_id),
    total_price NUMERIC(12, 2),  -- T·ªïng s·ªë ti·ªÅn c·ªßa ƒë∆°n h√†ng
    buy_count INT,  -- T·ªïng s·ªë l∆∞·ª£ng s·∫£n ph·∫©m c·ªßa ƒë∆°n h√†ng
    status INT,
    created_at TIMESTAMP,
    updated_at TIMESTAMP
);

CREATE TABLE shippings (
	shipping_id SERIAL PRIMARY KEY,
    purchase_id INT REFERENCES purchases(purchase_id),
    created_at TIMESTAMP,
    updated_at TIMESTAMP
)

CREATE TABLE refresh_tokens (
    refresh_token_id SERIAL PRIMARY KEY,
    token VARCHAR(255),
    user_id INT,
    created_at TIMESTAMP,
    expires_at TIMESTAMP,
    FOREIGN KEY (user_id) REFERENCES users(user_id) ON DELETE CASCADE
);


